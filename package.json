{
  "name": "caviar",
  "version": "3.0.0-alpha.5",
  "description": "A pluggable and extendable web framework which integrates React, Next and Koa to create your own web application out of the box with even zero configuration",
  "main": "src/index.js",
  "scripts": {
    "test": "nyc ava --timeout=30s --verbose",
    "test:only": "NODE_DEBUG=caviar nyc ava --timeout=30s --verbose",
    "lint": "eslint .",
    "posttest": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "report:dev": "nyc report --reporter=html && npm run report:open",
    "report:open": "open coverage/index.html",
    "test:dev": "npm run test && npm run report:dev && rm -rf node_modules/.cache"
  },
  "files": [
    "src/",
    "spawner/"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/kaelzhang/caviar.git"
  },
  "keywords": [
    "caviar",
    "skeleton",
    "frameworks",
    "micro-frontends",
    "frontends",
    "koa",
    "roe",
    "next",
    "webpack"
  ],
  "engines": {
    "node": ">= 6"
  },
  "ava": {
    "babel": false,
    "files": [
      "test/*.test.js"
    ]
  },
  "author": "kaelzhang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kaelzhang/caviar/issues"
  },
  "devDependencies": {
    "@babel/runtime-corejs2": "^7.4.5",
    "ava": "^2.1.0",
    "codecov": "^3.5.0",
    "eslint": "^6.0.1",
    "eslint-config-ostai": "^3.0.0",
    "eslint-plugin-import": "^2.18.0",
    "fs-extra": "^8.0.1",
    "get-port": "^5.0.0",
    "is-type-of": "^1.2.1",
    "next": "^8.1.0",
    "nyc": "^14.1.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "request": "^2.88.0",
    "roe-define-router": "^1.3.0",
    "supertest": "^4.0.2",
    "webpack": "^4.35.0"
  },
  "dependencies": {
    "core-util-is": "^1.0.2",
    "dotenv": "^8.0.0",
    "env-to-code": "^1.0.4",
    "err-object": "^5.1.2",
    "has-own-prop": "^2.0.0",
    "object-access": "^2.0.4",
    "once": "^1.4.0",
    "tapable": "^1.1.3",
    "tapable-proxy": "^1.0.3"
  }
}
